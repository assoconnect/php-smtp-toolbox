        "squizlabs/php_codesniffer": "^3",
        "assoconnect/phpstan-rules": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "require": {
        "php": "^7.4|^8.0",
        "symfony/cache": "^5.4|^6.0"
    }
